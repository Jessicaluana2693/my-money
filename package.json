{
  "name": "my-money",
  "version": "3.0.0",
  "description": "Investments dashboard app",
  "main": "index.js",
  "scripts": {
    "start": "node src/index",
    "test": "cross-env NODE_ENV=test mocha --recursive",
    "test:coverage": "nyc --reporter=html --reporter=text npm run test",
    "prettier": "prettier \"./{src,client/src}/**/*.{js,scss}\"",
    "prettier:check": "npm run prettier -- -l",
    "prettier:write": "npm run prettier -- --write",
    "lint": "eslint --fix \"./{src,client/src}/**/*.js\"",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doug2k1/my-money.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doug2k1/my-money/issues"
  },
  "homepage": "https://github.com/doug2k1/my-money#readme",
  "engines": {
    "node": "9.x.x"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0",
    "prettier": "^1.11.1",
    "sequelize-cli": "^4.0.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "forest-express-sequelize": "^2.5.6",
    "pg": "^7.4.1",
    "sequelize": "^4.37.3"
  },
  "nyc": {
    "exclude": ["config", "src/models/index.js", "test/**"]
  }
